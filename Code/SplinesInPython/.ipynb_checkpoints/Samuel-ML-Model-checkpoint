// Arthur Samuels View of A Machine Learning Model
digraph {
	rankdir=LR size="8,5"
	I [label=Inputs color="" rank=1 style=filled]
	W [label=Weights color="" rank=1 style=filled]
	M [label=Model color="" style=filled]
	R [label=Results color="" style=filled]
	P [label=Performance color="" style=filled]
	I -> M
	W -> M
	M -> R
	R -> P
	P -> W
}
